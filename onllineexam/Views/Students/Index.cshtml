                @Html.DisplayFor(modelItem => item.State)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Qualification)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Year_of_comp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Login.Type)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Stu_ID }) |
                @Html.ActionLink("Details", "Details", new { id=item.Stu_ID }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.Stu_ID })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
