@model IEnumerable<onllineexam.Models.QuestionData>
@{
    Layout = "~/Views/Shared/_TeacherLayout.cshtml";
}
<h2></h2>

<p>
    @Html.ActionLink("Add Question", "Create")
</p>
<div style="overflow:auto;">
<table class="table table-hover table-responsive table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Question)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.option1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.option2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.option3)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.option4)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.correct_option)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Level)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TestGenerator.Test_name)
        </th>
        @*<th></th>*@
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Question)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.option1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.option2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.option3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.option4)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.correct_option)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Level)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TestGenerator.Test_name)
            </td>
            @*<td>


                @*@Html.ActionLink("Delete", "Delete", new { id = item.testid, id1 = item.Question_number })*@
            </td>*@
        </tr>
    }

</table>
</div>