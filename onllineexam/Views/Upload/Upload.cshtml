@{
    Layout = "~/Views/Shared/_TeacherLayout.cshtml";
}<h2>Upload</h2>

@Session["exid"]
@using (Html.BeginForm("Upload", "Upload", FormMethod.Post, new
{
    enctype = "multipart/form-data"
}))
{
    <div>
        <input type="file" name="UploadedFile" />
    </div>
    <table>
        <tr>
            <td>File:</td>
            <td></td>
        </tr>
        <tr>
            <td colspan="2">
                <input type="submit"
                       name="Submit"
                       value="Submit" />
            </td>
        </tr>
    </table>
}
